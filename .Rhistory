length_outliers <- imdb_data %>%
group_by(rate) %>%
mutate(is_outlier = length > quantile(length, 0.75) + 1.5 * IQR(length) |
length < quantile(length, 0.25) - 1.5 * IQR(length)) %>%
filter(is_outlier) %>%
ungroup() # 97 outliers
#| label: tbl-summary-length
