group_by(rate) %>%
summarise('Mean' = mean(length),
'Median' = median(length),
'St.Dev' = sd(length),
'Min' = min(length),
'Max' = max(length),
'IQR' = quantile(length,0.75)-quantile(length,0.25),
'Sample_size' = n())
table|>
gt() |>
